{
  "manifest_version": 2,
  "name": "Quick Summary",
  "description": "This is a sample extension for demonstration purposes.",
  "version": "0.1",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_title": "My Extension",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
